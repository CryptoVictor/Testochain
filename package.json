{
  "name": "testochain-master",
  "version": "1.0.0",
  "description": "A cryptocurrency made for test",
  "main": "index.js",
  "scripts": {
    "test": "cross-env ENV='development' jest --watchAll",
    "start": "npm run build-client & node index.js",
    "dev": "run-p dev-client nodemon",
    "dev-peer": "cross-env GENERATE_PEER_PORT='true' ENV='development' nodemon index.js",
    "build-client": "npm run clean && parcel build client/src/index.html --out-dir client/dist",
    "dev-client": "run-s clean parcel",
    "clean": "if exist .cache (rmdir /s/q .cache) && if exist client\\dist (rmdir /s/q client\\dist)",
    "nodemon": "cross-env ENV='development' nodemon index.js",
    "parcel": "parcel client/src/index.html --out-dir client/dist"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "keywords": [],
  "author": "Victor Garcia | Special Thanks: David Joseph Katz",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-react": "^7.16.7",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.2.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "elliptic": "^6.4.1",
    "express": "^4.16.3",
    "hex-to-binary": "^1.0.1",
    "history": "^4.7.2",
    "parcel-bundler": "^1.12.5",
    "pubnub": "^4.21.6",
    "react": "^16.6.0",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.6.0",
    "react-router-dom": "^4.3.1",
    "request": "^2.88.0",
    "uuid": "^3.3.2"
  }
}
